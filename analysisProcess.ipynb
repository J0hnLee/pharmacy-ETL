    "import pulp\n",
    "\n",
    "#目标函数z的系数\n",
    "z = [-1,2,-3]\n",
    "\n",
    "#不等式约束条件，也要转化为 ≤的形式\n",
    "A_ub = [[1, 1, 1], [-1, 1, -1]]\n",
    "b_ub = [7, -2]\n",
    "\n",
    "#等式约束条件\n",
    "A_eq = [[-3,1,2]]\n",
    "b_eq = [5]"
   ]
  },
